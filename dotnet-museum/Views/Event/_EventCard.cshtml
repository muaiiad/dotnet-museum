@model dotnet_museum.Models.MuseumEvents.EventModel

<div class="card">
    <img src="@Model.ImagePath" class="card-img-top" alt="@Model.Title">
    <div class="card-body">
        <h5 class="card-title"> @Model.Title </h5>
        <p class="card-text"> @Model.Description </p>
        <a asp-action="Details" asp-route-id="@Model.EventId" class="btn btn-primary">View Details</a>
    </div>
</div>